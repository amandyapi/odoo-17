<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">
        <record id="sequence_i_contact" model="ir.sequence">
            <field name="name">Contact Sequence</field>
            <field name="code">i_contact_sequence</field>
            <!-- Define your sequence properties like prefix, suffix, padding, etc. -->
            <field name="prefix">MA/</field>
            <field name="padding">4</field>
            <field name="suffix">/%(year)s</field>
            <!-- Add other sequence properties -->
        </record>
        
        <record id="sequence_res_partner" model="ir.sequence">
            <field name="name">Partner Sequence</field>
            <field name="code">res_partner_sequence</field>
            <!-- Define your sequence properties like prefix, suffix, padding, etc. -->
            <field name="prefix">RP/</field>
            <field name="padding">4</field>
            <field name="suffix">/%(year)s</field>
            <!-- Add other sequence properties -->
        </record>
    </data>
</odoo>
 